{
  "$schema": "https://json.schemastore.org/package",
  "name": "e-commerce-solutions",
  "private": true,
  "version": "2.4.1",
  "type": "module",
  "scripts": {
    "dev": "node server.js",
    "dev:client": "vite",
    "ensure:env": "node scripts/ensure-env.mjs",
    "build": "npm run ensure:env && npm run build:client && npm run build:server && npm run copy:html",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr entry-server.tsx --outDir dist/server",
    "copy:html": "node scripts/copy-html-backup.js",
    "start": "NODE_ENV=production node server.js",
    "preview": "vite preview",
    "test": "vitest run",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@google/genai": "^0.7.0",
    "@google/generative-ai": "^0.24.1",
    "@tensorflow-models/mobilenet": "^2.1.1",
    "@tensorflow/tfjs": "^4.22.0",
    "@upstash/redis": "^1.32.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "dotenv": "^16.4.7",
    "express": "^4.22.1",
    "ioredis": "^5.9.0",
    "lucide-react": "^0.263.1",
    "mongodb": "^6.11.0",
    "mongoose": "^8.8.3",
    "multer": "^1.4.5-lts.1",
    "papaparse": "^5.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hot-toast": "^2.5.1",
    "react-loading-skeleton": "^3.5.0",
    "recharts": "2.12.0",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.1",
    "tailwind": "^2.3.1",
    "uuid": "^13.0.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.6",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.14.0",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^19.2.13",
    "@types/socket.io": "^3.0.1",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.1.2",
    "jsdom": "^26.0.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "vite": "^6.4.1",
    "vitest": "^4.0.17"
  }
}
